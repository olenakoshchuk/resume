<div>
    {{#with main}}
        <main>
            <section>
                {{> title big=true data="My educationss" }}
                <ul class="ps-5">
                    {{#each education}}
                        <li>
                            {{name}}
                            {{#unless isEnd}} (ще в процесі) {{/unless}}
                        </li>
                    {{/each}}
                </ul>
            </section>
            <section>
                {{> title big=false data="My certificates" }}
                    <ul class="ps-5">
                        {{#each certificates}}
                            <li>
                                {{name}} ({{id}})
                            </li>
                        {{/each}}
                    </ul>
            </section>
        </main>
    {{/with}}
</div>